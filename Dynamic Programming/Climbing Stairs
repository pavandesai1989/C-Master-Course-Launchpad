#include<iostream>
#include<vector>
using namespace std;
/*
 Time complexity : O(N)
 Space complexity : O(N)
 */

int NoofWays(int N)
{
    vector<int> dp(N+1,-1);
    dp[0] = 1;
    dp[1] = 1;
    dp[2] = 2;
    
    for(int i=3;i<=N;i++)
    {
        dp[i] = dp[i-1] + dp[i-2];
    }
    
    return dp[N];
}



int main()
{
    cout<<NoofWays(5)<<endl;
    return 0;
}
